extends layout

block content
  h1= title
  if item_list.length
    ul
      each item in item_list
        li
          a(href=item.url) #{item.name}
            each val, index in item.category
              a  (#{val.name})
              if index < item.category.length - 1
                |,&nbsp;
            

  else
    p There are no items.
